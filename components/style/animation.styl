@keyframes pilipaFadeIn
  0%
    opacity: 0
  100%
    opacity: 1;
@keyframes pilipaFadeOut
  0%
    opacity: 1
  100%
    opacity: 0
@keyframes pilipaZoomIn
  0%
    opacity: 0
    top: 0
  100%
    opacity: 1
    top: 100px
@keyframes pilipaZoomOut
  0%
    opacity: 1
    top: 100px
  100%
    opacity: 0
    top: 0
@keyframes pilipaNotificationFadeIn
  0%
    opacity: 0;
    right: -384px
  100%
    opacity: 1;
    right: 0
@keyframes pilipaNotificationFadeOut
  0%
    opacity: 1;
    margin-bottom: 16px;
    padding-top: 16px 24px;
    padding-bottom: 16px 24px;
    max-height: 150px;
  100%
    opacity: 0;
    margin-bottom: 0;
    padding-top: 0;
    padding-bottom: 0;
    max-height: 0;
@keyframes pilipaRingToggle
  0%
    box-shadow: 0 0 10px #FFF;
    // transform: scale(.95)
  50%
    box-shadow: 0 0 30px #FFF;
    // transform: scale(1)
  100%
    box-shadow: 0 0 10px #FFF;
    // transform: scale(.95)
@keyframes pilipaWebUploadFadeIn 
  0%
    right: 0
    opacity: 0
  70%
    right: 14px
    opacity: 1
  100%
    right: 10px
    opacity: 1